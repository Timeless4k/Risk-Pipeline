{
    "data": {
        "start_date": "2000-01-01",
        "end_date": "2025-01-05",
        "us_assets": ["AAPL", "MSFT", "^GSPC"],
        "au_assets": ["IOZ.AX", "CBA.AX", "BHP.AX"],
        "cache_dir": "data_cache"
    },
    "features": {
        "volatility_window": 5,
        "ma_short": 10,
        "ma_long": 50,
        "correlation_window": 30,
        "sequence_length": 15
    },
    "models": {
        "lstm_units": [100, 50],
        "lstm_dropout": 0.2,
        "stockmixer_temporal_units": 128,
        "stockmixer_indicator_units": 128,
        "stockmixer_cross_stock_units": 128,
        "stockmixer_fusion_units": 256,
        "xgboost_n_estimators": 200,
        "xgboost_max_depth": 8,
        "xgboost_learning_rate": 0.05,
        "arima_order": [2, 1, 2]
    },
    "training": {
        "walk_forward_splits": 10,
        "test_size": 63,
        "batch_size": 32,
        "epochs": 200,
        "early_stopping_patience": 10,
        "reduce_lr_patience": 8,
        "random_state": 42
    },
    "output": {
        "results_dir": "results",
        "plots_dir": "visualizations",
        "shap_dir": "shap_plots",
        "models_dir": "models",
        "log_dir": "logs"
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
        "date_format": "%Y-%m-%d %H:%M:%S"
    },
    "shap": {
        "background_samples": 200,
        "max_display": 30,
        "plot_type": "bar",
        "save_plots": true
    }
} 